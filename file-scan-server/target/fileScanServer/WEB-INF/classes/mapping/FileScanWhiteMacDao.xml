<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yzy.dao.FileScanWhiteMacDao">
  <select id="isInWhiteMacList" parameterType="string" resultType="int">
    select count(1)
    from file_scan_white_list
    where mac=#{mac}
  </select>
</mapper>